{
  "name": "vite-plugin-browserslist-useragent",
  "version": "0.0.3",
  "description": "A utility vite plugin to compile browserslist query to a RegExp to test browser useragent.",
  "author": "Yue JIN <yuejin13@fudan.edu.cn>",
  "packageManager": "pnpm@7.10.0",
  "engines": {
    "node": ">=14"
  },
  "license": "MIT",
  "keywords": [
    "browserslist",
    "regexp",
    "vite-plugin"
  ],
  "main": "index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "client.d.ts"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kingyue737/vite-plugin-browserslist-useragent.git"
  },
  "homepage": "https://github.com/kingyue737/vite-plugin-browserslist-useragent#readme",
  "bugs": {
    "url": "https://github.com/kingyue737/vite-plugin-browserslist-useragent/issues"
  },
  "scripts": {
    "build": "tsup src/index.ts --dts --format cjs,esm",
    "prepublishOnly": "npm run build",
    "release": "bumpp --execute=\"pnpm changelog\" --commit \"ðŸ”– Release v\" --push --tag --all && npm publish --registry=\"https://registry.npmjs.org/\"",
    "changelog": "gitmoji-changelog"
  },
  "dependencies": {
    "browserslist-useragent-regexp": "^3.0.2"
  },
  "devDependencies": {
    "bumpp": "^8.2.1",
    "gitmoji-changelog": "^2.3.0",
    "prettier": "^2.7.1",
    "tsup": "^6.1.3",
    "typescript": "^4.7.4",
    "vite": "^3.0.2"
  },
  "peerDependencies": {
    "vite": "^2.5.0 || ^3.0.0"
  },
  "pnpm": {
    "overrides": {
      "useragent": "npm:@kingyue/useragent@^2.4.0"
    }
  }
}
